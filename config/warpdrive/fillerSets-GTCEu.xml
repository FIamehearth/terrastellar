    <!-- overworld variations -->
	<for variable="vein" in="apatite,cassiterite,coal,copper_tin,copper,diamond,galena,garnet_tin,garnet,iron,lapis,lubricant,magnetite,manganese,mica,mineral_sand,nickel,oilsands,olivine,redstone,salts,sapphire">
		
		<!-- common overworld ores with various base blocks -->
		<fillerSet group="overworld_common" name="%vein%-standstone" weight="2">
			<import group="overworld_common" name="%vein%" /><!-- import the smooth version -->
			<filler block="minecraft:sandstone"      metadata="0" weight="100" />
			<filler block="minecraft:clay"           metadata="0" weight="100" />
		</fillerSet>
		<fillerSet group="overworld_common" name="%vein%-cobbleLight" weight="2">
			<import group="overworld_common" name="%vein%" /><!-- import the smooth version -->
			<filler block="minecraft:cobblestone"    metadata="0" weight="2" />
		</fillerSet>
		<fillerSet group="overworld_common" name="%vein%-cobbleHeavy" weight="2">
			<import group="overworld_common" name="%vein%" /><!-- import the smooth version -->
			<filler block="minecraft:cobblestone"    metadata="0" weight="10" />
		</fillerSet>
		
		<!-- uncommon overworld veins are just a flat distribution of common veins -->
		<fillerSet group="overworld_uncommon" name="%vein%" weight="10">
			<import group="overworld_common" name="%vein%" /><!-- import the common version -->
		</fillerSet>
	</for>
	
	
	
