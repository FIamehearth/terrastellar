				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_lepidolite_0"            metadata="%metadata%" ratio="0.0500" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_spodumene_0"             metadata="%metadata%" ratio="0.0100" />
