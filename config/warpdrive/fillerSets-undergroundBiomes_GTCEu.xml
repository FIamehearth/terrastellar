			<!-- garnet vein: density 0.25 * {0.35 0.35 0.25 0.05} -->
			<fillerSet group="overworld_common" name="garnet-%type%@%metadata%" weight="10">
				<filler block="undergroundbiomes:%type%_stone"                                      metadata="%metadata%" weight="1" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_garnet_red_0"            metadata="%metadata%" ratio="0.0875" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_garnet_yellow_0"         metadata="%metadata%" ratio="0.0875" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_amethyst_0"              metadata="%metadata%" ratio="0.0625" />
