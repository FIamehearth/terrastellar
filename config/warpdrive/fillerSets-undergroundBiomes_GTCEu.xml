			<fillerSet group="overworld_common" name="olivine-%type%@%metadata%" weight="20">
				<filler block="undergroundbiomes:%type%_stone"                                      metadata="%metadata%" weight="1" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_bentonite_0"             metadata="%metadata%" ratio="0.0875" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_magnesite_0"             metadata="%metadata%" ratio="0.0875" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_olivine_0"               metadata="%metadata%" ratio="0.0625" />
