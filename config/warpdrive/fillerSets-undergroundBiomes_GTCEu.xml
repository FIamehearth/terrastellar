    <!-- corrupted versions (non-cobblestone) -->
    <for variable="type" in="igneous,metamorphic,sedimentary">
        <for variable="metadata" from="0" to="7">
			<for variable="vein" in="apatite,cassiterite,coal,copper_tin,copper,diamond,galena,garnet_tin,garnet,iron,lapis,lubricant,magnetite,manganese,mica,mineral_sand,nickel,oilsands,olivine,redstone,salts,sapphire">
				
				<!-- common overworld ores with various base blocks -->
				<fillerSet group="overworld_common" name="%vein%-%type%@%metadata%-standstone" weight="5">
					<import group="overworld_common" name="%vein%-%type%@%metadata%" /><!-- import the smooth version -->
					<filler block="undergroundbiomes:%type%_sandstone" metadata="%metadata%" weight="100" />
					<filler block="undergroundbiomes:%type%_clay"      metadata="%metadata%" weight="100" />
				</fillerSet>
			</for>
        </for>
    </for>
	
    <!-- corrupted versions (no sedimentary cobblestone) -->
    <for variable="type" in="igneous,metamorphic">
        <for variable="metadata" from="0" to="7">
			<for variable="vein" in="apatite,cassiterite,coal,copper_tin,copper,diamond,galena,garnet_tin,garnet,iron,lapis,lubricant,magnetite,manganese,mica,mineral_sand,nickel,oilsands,olivine,redstone,salts,sapphire">
				
				<!-- common overworld ores with various base blocks -->
				<fillerSet group="overworld_common" name="%vein%-%type%@%metadata%-cobbleLight" weight="2">
					<import group="overworld_common" name="%vein%-%type%@%metadata%" /><!-- import the smooth version -->
					<filler block="undergroundbiomes:%type%_cobble"    metadata="%metadata%" weight="2" />
				</fillerSet>
				<fillerSet group="overworld_common" name="%vein%-%type%@%metadata%-cobbleHeavy" weight="2">
					<import group="overworld_common" name="%vein%-%type%@%metadata%" /><!-- import the smooth version -->
					<filler block="undergroundbiomes:%type%_cobble"    metadata="%metadata%" weight="10" />
				</fillerSet>
				
				<!-- uncommon overworld veins are just a flat distribution of common veins -->
				<fillerSet group="overworld_uncommon" name="%vein%-%type%@%metadata%" weight="10">
					<import group="overworld_common" name="%vein%-%type%@%metadata%" /><!-- import the common version -->
				</fillerSet>
			</for>
			
			<!-- assorted overworld surface blocks -->
			<fillerSet group="overworld_surface" name="%type%@%metadata%-cobble" weight="2">
				<filler block="undergroundbiomes:%type%_cobble" metadata="%metadata%" weight="100" />
			</fillerSet>
        </for>
    </for>
	
</worldGeneration>