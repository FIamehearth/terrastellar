			<!-- mineral_sand vein: density 0.2 * {0.35 0.35 0.25 0.05} -->
			<fillerSet group="overworld_common" name="mineral_sand-%type%@%metadata%" weight="80">
				<filler block="undergroundbiomes:%type%_stone"                                      metadata="%metadata%" weight="1" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_basaltic_mineral_sand_0" metadata="%metadata%" ratio="0.0700" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_granitic_mineral_sand_0" metadata="%metadata%" ratio="0.0700" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_fullers_earth_0"         metadata="%metadata%" ratio="0.0500" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_gypsum_0"                metadata="%metadata%" ratio="0.0100" />
			</fillerSet>
			
			<!-- nickel vein: density 0.25 * {0.35 0.35 0.25 0.05} -->
			<fillerSet group="overworld_common" name="nickel-%type%@%metadata%" weight="40">
				<filler block="undergroundbiomes:%type%_stone"                                      metadata="%metadata%" weight="1" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_garnierite_0"            metadata="%metadata%" ratio="0.0875" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_nickel_0"                metadata="%metadata%" ratio="0.0875" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_cobaltite_0"             metadata="%metadata%" ratio="0.0625" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_pentlandite_0"           metadata="%metadata%" ratio="0.0125" />
			</fillerSet>
			
			<!-- oilsands vein: density 0.3 * {0.35 0.35 0.25 0.05} -->
			<fillerSet group="overworld_common" name="oilsands-%type%@%metadata%" weight="40">
				<filler block="undergroundbiomes:%type%_stone"                                      metadata="%metadata%" weight="1" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_oilsands_0"              metadata="%metadata%" ratio="0.3000" />
