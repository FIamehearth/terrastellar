				<filler block="undergroundbiomes:%type%_stone"                                      metadata="%metadata%" weight="1" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_soapstone_0"             metadata="%metadata%" ratio="0.0875" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_talc_0"                  metadata="%metadata%" ratio="0.0875" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_glauconite_sand_0"       metadata="%metadata%" ratio="0.0625" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_pentlandite_0"           metadata="%metadata%" ratio="0.0125" />
			</fillerSet>
			
			<!-- magnetite vein: density 0.15 * {0.35 0.35 0.25 0.05} -->
			<fillerSet group="overworld_common" name="magnetite-%type%@%metadata%" weight="80">
				<filler block="undergroundbiomes:%type%_stone"                                      metadata="%metadata%" weight="1" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_magnetite_0"             metadata="%metadata%" ratio="0.1050" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_vanadium_magnetite_0"    metadata="%metadata%" ratio="0.0375" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_gold_0"                  metadata="%metadata%" ratio="0.0075" />
