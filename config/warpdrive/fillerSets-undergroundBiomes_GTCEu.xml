<?xml version="1.0" encoding="utf-8"?>
<worldGeneration version="3"
				 xmlns="http://warpdrive.lemadec.org"
				 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				 xsi:schemaLocation="http://warpdrive.lemadec.org WarpDrive.xsd"
				 mods="undergroundbiomes,gregtech">

	<for variable="type" in="igneous,metamorphic,sedimentary">
		<for variable="metadata" from="0" to="7">
			<!-- note: we can't dynamically import from a FillerSet, so we have explicitly declare 1 filler set per GTCEu vein -->
			
			<!-- apatite vein: density 0.25 * {0.35 0.35 0.25 0.05} -->
			<fillerSet group="overworld_common" name="apatite-%type%@%metadata%" weight="40">
				<filler block="undergroundbiomes:%type%_stone"                                      metadata="%metadata%" weight="1" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_apatite_0"               metadata="%metadata%" ratio="0.1750" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_tricalcium_phosphate_0"  metadata="%metadata%" ratio="0.0625" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_pyrochlore_0"            metadata="%metadata%" ratio="0.0125" />
			</fillerSet>
			
			<!-- cassiterite vein: density 0.2 * {0.35 0.35 0.25 0.05} -->
			<fillerSet group="overworld_common" name="cassiterite-%type%@%metadata%" weight="50">
				<filler block="undergroundbiomes:%type%_stone"                                      metadata="%metadata%" weight="1" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_tin_0"                   metadata="%metadata%" ratio="0.1500" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_cassiterite_0"           metadata="%metadata%" ratio="0.0500" />
