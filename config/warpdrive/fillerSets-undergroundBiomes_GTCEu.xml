				<filler block="undergroundbiomes:%type%_stone"                                      metadata="%metadata%" weight="1" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_grossular_0"             metadata="%metadata%" ratio="0.0875" />
