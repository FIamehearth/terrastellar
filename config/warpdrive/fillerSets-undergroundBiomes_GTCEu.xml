			</fillerSet>
			
			<!-- mica vein: density 0.25 * {0.35 0.35 0.25 0.05} -->
			<fillerSet group="overworld_common" name="mica-%type%@%metadata%" weight="20">
				<filler block="undergroundbiomes:%type%_stone"                                      metadata="%metadata%" weight="1" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_kyanite_0"               metadata="%metadata%" ratio="0.0875" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_mica_0"                  metadata="%metadata%" ratio="0.0875" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_bauxite_0"               metadata="%metadata%" ratio="0.0625" />
				<filler block="undergroundbiomes:%type%_stone_gregtech_ore_pollucite_0"             metadata="%metadata%" ratio="0.0125" />
